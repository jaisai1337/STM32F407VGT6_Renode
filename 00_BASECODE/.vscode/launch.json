{
    "version": "0.2.0",
     "configurations": [
        {
            "name": "attach to renode",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/BASECODE.elf",
            "args": [],
            "svdPath" : "${workspaceRoot}/STM32F407.svd",
            "stopAtEntry": true,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true,
                }
            ],
            "miDebuggerPath": "/opt/gcc-arm-none-eabi-10.3-2021.10/bin/arm-none-eabi-gdb",
            "miDebuggerServerAddress": "localhost:1234",
            // "preLaunchTask": "upload to renode",
            // "postDebugTask": "quit renode"
        }
    ]
} 